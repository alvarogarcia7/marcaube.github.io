---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>marcaube.ca</title>
        <link href="http://marcaube.ca/"/>
        <atom:link href="http://marcaube.ca/feed.xml" rel="self" type="application/rss+xml" />
    
        {% for post in site.posts %}
            <item>
                <title>{{ post.title }}</title>
                <description>{{ post.content | xml_escape }}</description>
                <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
                <link>http://marcaube.ca{{ post.url }}"</lin>
                <guid isPermaLink="true">http://marcaube.ca{{ post.id }}</guid>
            </item>
        {% endfor %}
    </channel>
</rss>
